                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>
                                    Search By Keyword
                                </label>
                                <div class="input-group">
                                    @Html.TextBoxFor(model => model.UserTypeId, new { @placeholder = "Search by Role", @class = "form-control", @type = "text", @maxlength = "100" })
