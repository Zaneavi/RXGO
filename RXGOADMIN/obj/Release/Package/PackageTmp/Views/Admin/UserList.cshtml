
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="page-breadcrumb">
    <div class="row">
        <div class="col-12 d-flex no-block align-items-center">
            <h4 class="page-title">
                User List
            </h4>
            <div class="ml-auto text-right">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/Admin">Home</a></li>
                        <li class="breadcrumb-item">
                            <a href="javascript:(void);">
                                Master Admin
                            </a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">
                            User List
                        </li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="zero_config" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>User Type</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (ViewBag.UserList.Count > 0)
                                {
                                    foreach (var i in ViewBag.UserList)
                                    {
                                        <tr>

                                            <td>@i.UserType</td>
                                            @if (i.IsActive == "True")
                                            {
                                                <td><a href="javascript:void(0)" onclick="ChngStatus('@i.UserTypeId','@i.IsActive')" class="btn btn-success btn-xs">ACTIVE</a></td>
                                            }
                                            else
                                            {
                                                <td><a href="javascript:void(0)" onclick="ChngStatus('@i.UserTypeId','@i.IsActive')" class="btn btn-danger btn-xs">INACTIVE</a></td>
                                            }
                                        </tr>
                                    }
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Type</th>
                                    <th>Status</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

